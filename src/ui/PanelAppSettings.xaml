<UserControl x:Class="NAMEGEN.Ui.PanelAppSettings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NAMEGEN.Control"
             mc:Ignorable="d" 
             d:DesignHeight="120" d:DesignWidth="830">

    <UserControl.DataContext>
        <local:ViewModel/>
    </UserControl.DataContext>

    <!-- delete later -->
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="DictionaryStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>


    <Canvas Name="canvasPanelAppSettings">
        <Rectangle
                Name="bgOverlayAppSettings"
                Height="120"
                Width="830" 
                Fill="Lavender"
                Canvas.ZIndex="0" />

    </Canvas>
</UserControl>
